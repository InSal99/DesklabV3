<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomFooter">
        <attr name="footerType" format="enum">
            <enum name="CALL_TO_ACTION" value="0" />
            <enum name="CALL_TO_ACTION_DETAIL" value="1" />
            <enum name="DUAL_BUTTON" value="2" />
            <enum name="NO_ACTION" value="3" />
        </attr>

        <attr name="footerHasStroke" format="boolean" />

        <attr name="primaryButtonText" format="string" />
        <attr name="secondaryButtonText" format="string" />

        <attr name="primaryButtonEnabled" format="boolean" />
        <attr name="secondaryButtonEnabled" format="boolean" />

        <attr name="footerTitle" format="string" />
        <attr name="footerDescription" format="string" />

        <attr name="statusBadgeText" />
        <attr name="statusBadgeType" />
    </declare-styleable>
</resources>