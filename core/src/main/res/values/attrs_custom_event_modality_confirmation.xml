<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomEventModalityConfirmation">
        <!-- Attribute for the main title of the modal -->
        <attr name="modalTitle" format="string" />
        <!-- Attribute for the descriptive text of the modal -->
        <attr name="modalDescription" format="string" />
        <!-- Attribute for the confirmation button's label -->
        <attr name="modalConfirmButtonLabel" format="string" />
        <!-- Attribute for the close/cancel button's label -->
        <attr name="modalCloseButtonLabel" format="string" />
    </declare-styleable>
</resources>
