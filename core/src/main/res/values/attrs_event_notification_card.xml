<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="EventNotificationCard">
        <attr name="notificationTitle" format="string" />
        <attr name="notificationDescription" format="string" />
        <attr name="notificationButtonVisible" format="boolean" />
        <attr name="notificationButtonText" format="string" />
        <attr name="notificationEventCategory" format="enum">
            <enum name="general_event" value="0" />
            <enum name="people_development" value="1" />
            <enum name="employee_benefit" value="2" />
        </attr>
    </declare-styleable>
</resources>