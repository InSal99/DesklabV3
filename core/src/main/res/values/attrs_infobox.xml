<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="InfoBox">
        <attr name="text" format="string"/>
        <attr name="infoboxType" format="enum">
            <enum name="information" value="0"/>
            <enum name="success" value="1"/>
            <enum name="error" value="2"/>
            <enum name="general" value="3"/>
        </attr>
    </declare-styleable>
</resources>