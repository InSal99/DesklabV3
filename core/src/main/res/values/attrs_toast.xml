<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomToast">
        <attr name="toastType" format="enum">
            <enum name="success" value="0" />
            <enum name="error" value="1" />
            <enum name="info" value="2" />
            <enum name="general" value="3" />
        </attr>
        <attr name="toastMessage" format="string" />
    </declare-styleable>
</resources>