<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomMonthlyPicker">
        <attr name="monthLabel" format="string" />
        <attr name="pickerType" format="enum">
            <enum name="unselected" value="0" />
            <enum name="selected" value="1" />
            <enum name="disabled" value="2" />
        </attr>
    </declare-styleable>
</resources>